@{ Layout = null; }
<!DOCTYPE html>

<html>
<head>
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
    <link rel="stylesheet" type="text/css" href="~/css/loginpage.css" />
    <script type="text/javascript" src="~/js/loginpage.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title>Login Page</title>
</head>
<body>
    <div>
        <label>User Name : </label><input id="txtUserName" type="text" />
        <label>Password  : </label><input id="txtPassword" type="password" /> <label id="errMsg"></label><br />
        <button id="btnLogin" onclick="user.validateUser()">Login</button>
        <button id="btnOpenPopup" onclick="user.openPopup()">SignUp</button>
        <div id="userPopup" class="hide">
            @{ Html.RenderPartial("~/Views/Shared/_AddUser.cshtml"); }
        </div>
    </div>
</body>
</html>